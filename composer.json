{
    "name": "forkcms/forkcms",
    "description": "Fork is an open source CMS that will rock your world.",
    "keywords": [
        "cms",
        "fork",
        "fork cms",
        "content management system",
        "symfony",
        "blog",
        "php"
    ],
    "homepage": "http://www.fork-cms.com/",
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "ext-dom": "*",
        "ext-fileinfo": "*",
        "ext-json": "*",
        "ext-zip": "*",
        "behat/transliterator": "~1.0",
        "doctrine/doctrine-bundle": "^2.1",
        "doctrine/orm": "^2.7",
        "google/apiclient": "~1.1.2",
        "google/recaptcha": "~1.1",
        "jeroendesloovere/geolocation-php-api": "^2.1",
        "league/flysystem": "~1.0",
        "league/flysystem-aws-s3-v3": "^1.0.13",
        "league/flysystem-cached-adapter": "^1.0.6",
        "liip/imagine-bundle": "^2.3",
        "matthiasmullie/minify": "~1.3",
        "matthiasmullie/scrapbook": "^1.3",
        "pimple/pimple": "^3.2",
        "ramsey/uuid": "^3.5",
        "ramsey/uuid-doctrine": "^1.2",
        "simple-bus/doctrine-orm-bridge": "^5.1",
        "simple-bus/symfony-bridge": "^5.4",
        "spoon/library": "^3.0",
        "swiftmailer/swiftmailer": "^6.0",
        "symfony/monolog-bundle": "^3.1",
        "symfony/swiftmailer-bundle": "^3.0",
        "symfony/symfony": "^5",
        "tijsverkoyen/akismet": "1.1.*",
        "tijsverkoyen/css-to-inline-styles": "^2.2",
        "friendsofsymfony/rest-bundle": "^3.0",
        "jms/serializer-bundle": "^3.7",
        "codex-team/editor.js": "^2.0",
        "beberlei/doctrineextensions": "^1.2",
        "symfony/webpack-encore-bundle": "^1.7",
        "mailmotor/mailmotor-bundle": "dev-master#8281809e932efe3e53cfcf284ba12d9dd7356b9e"
    },
    "require-dev": {
        "jdorn/sql-formatter": "1.2.17",
        "squizlabs/php_codesniffer": "^3.5",
        "symfony/phpunit-bridge": "^3.2",
        "phpstan/phpstan": "^0.12.47",
        "phpstan/phpstan-symfony": "^0.12.7",
        "symfony/debug-bundle": "^5.1",
        "symfony/var-dumper": "^5.1",
        "symfony/profiler-pack": "^1.0"
    },
    "config": {
        "bin-dir": "bin"
    },
    "support": {
        "forum": "https://fork-cms.herokuapp.com",
        "issues": "https://github.com/forkcms/forkcms/issues"
    },
    "scripts": {
        "test": "./bin/simple-phpunit"
    },
    "autoload": {
        "psr-4": {
            "Frontend\\": "src/Frontend",
            "Backend\\": "src/Backend",
            "Common\\": "src/Common",
            "Console\\": "src/Console",
            "ForkCMS\\": "src/ForkCMS",
            "ForkCMS\\App\\": "app"
        },
        "exclude-from-classmap": [
            "**/Tests/",
            "**/tests/",
            "**/Test/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "ForkCMS\\Tests\\": "tests/"
        }
    },
    "repositories":[
        {
            "type": "vcs",
            "url": "git@github.com:StijnVrolijk/mailmotor-bundle.git"
        }
    ]
}
